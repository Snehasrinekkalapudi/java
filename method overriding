import java.util.*;
class Vehicle{
  void run(){
    System.out.println("Vehicle is running");
  }
  }
class Bike extends Vehicle{
    void run(){
   System.out.println("bike is running");
}
public static void main(String args[]){
   Bike b=new Bike();
   Vehicle v=new Vehicle();
   b.run();
   v.run();
}
}
